/**
 * Definition for singly-linked list.
 * public class ListNode {
 *     int val;
 *     ListNode next;
 *     ListNode(int x) { val = x; }
 * }
 */
public class Solution {
    public ListNode partition(ListNode head, int x) {
        ListNode newHead = new ListNode(Integer.MIN_VALUE);
        ListNode highStart = newHead.next, lowStart = newHead;
        while(highStart != null && highStart.val < x) {
            lowStart = lowStart.next;
            highStart = highStart.next;
        }
        if(highStart == null)
            return newHead.next;
        ListNode high = highStart;
        ListNode temp = high.next;
        while(temp != null) {
            if(temp.val < x) {
                lowStart.next = temp;
                lowStart = lowStart.next;
                temp = temp.next;
            }
            else {
                high.next = temp;
                high = high.next;
                temp = temp.next;
            }
        }
        lowStart.next = highStart;
        high.next = null;
        return newHead.next;
    }
}
